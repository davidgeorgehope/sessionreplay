# Copy this file to .env and fill in your values
# .env is gitignored and won't be committed

# OTLP Endpoint (from Elastic Cloud: Add data → Applications → OpenTelemetry)
OTEL_EXPORTER_OTLP_ENDPOINT=https://your-otlp-endpoint.elastic.cloud:443

# API Key for OTLP authentication (Base64 encoded, must include "ApiKey" prefix)
OTEL_EXPORTER_OTLP_HEADERS=Authorization=ApiKey YOUR_API_KEY_HERE

# Service metadata
OTEL_RESOURCE_ATTRIBUTES=service.name=session-replay-demo,deployment.environment=development

# Elasticsearch endpoint (for verification scripts)
# Tip: Replace .kb. with .es. from your Kibana URL
ES_ENDPOINT=https://your-deployment.es.us-east-1.aws.elastic.cloud:443
ES_API_KEY=YOUR_API_KEY_HERE

# Kibana URL (for dashboard setup)
KIBANA_URL=https://your-deployment.kb.us-east-1.aws.elastic.cloud
