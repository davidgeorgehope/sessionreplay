# Copy this file to .env and fill in your values
# .env is gitignored and won't be committed

# Elastic Cloud OTLP endpoint (for sending telemetry)
OTEL_EXPORTER_OTLP_ENDPOINT=https://your-cluster.ingest.us-east-1.aws.elastic.cloud:443

# API Key for OTLP authentication
OTEL_EXPORTER_OTLP_HEADERS=Authorization=ApiKey YOUR_OTLP_API_KEY_HERE

# Service metadata
OTEL_RESOURCE_ATTRIBUTES=service.name=session-replay-demo,service.version=0.0.1,deployment.environment=development

# Elasticsearch Query API (for verification scripts)
ES_ENDPOINT=https://your-cluster.es.us-east-1.aws.elastic.cloud:443
ES_API_KEY=YOUR_ES_API_KEY_HERE

# Kibana URL (for manual verification)
KIBANA_URL=https://your-cluster.kb.us-east-1.aws.elastic.cloud
